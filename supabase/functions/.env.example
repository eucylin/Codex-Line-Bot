# Supabase Edge Functions Environment Variables
# Copy this file to .env.local and fill in your values

# LINE Bot Configuration
# Get these from LINE Developers Console: https://developers.line.biz/console/
LINE_CHANNEL_SECRET=your_line_channel_secret_here
LINE_CHANNEL_ACCESS_TOKEN=your_line_channel_access_token_here
LINE_BOT_NAME=your_bot_display_name_here

# Supabase Configuration (automatically set in production)
# For local development, these are provided by `supabase start`
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
